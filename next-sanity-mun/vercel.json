{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SANITY_PROJECT_ID": "ny2z8d3s",
    "NEXT_PUBLIC_SANITY_DATASET": "production",
    "NEXT_PUBLIC_SANITY_API_VERSION": "2024-03-19",
    "SANITY_API_TOKEN": "$SANITY_API_TOKEN"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_SANITY_PROJECT_ID": "ny2z8d3s",
      "NEXT_PUBLIC_SANITY_DATASET": "production",
      "NEXT_PUBLIC_SANITY_API_VERSION": "2024-03-19",
      "SANITY_API_TOKEN": "$SANITY_API_TOKEN"
    }
  }
}